    "jest-environment-jsdom-sixteen": "1.0.3",
    "prettier": "2.0.5",
    "react-currency-input": "^1.3.6",
    "sinon": "^9.0.3",
    "utils": "^0.3.1",
    "wait-on": "^5.0.0"
