  "scripts": {
    "start": "react-scripts start",
    "prettier": "prettier --write 'src/**/*.js' 'src/**/*.jsx'",
    "lint": "eslint --ext .jsx --ext .js src",
    "build": "npm install && react-scripts build",
    "coverage": "npm install && CI=true react-scripts test --env=jsdom --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/**/*Styles.js --collectCoverageFrom=!src/**/index.js --collectCoverageFrom=!src/**/AppRoutes.js --collectCoverage true --coverageDirectory ./.coverage",
    "localcoverage": "react-scripts test --env=jsdom --collectCoverageFrom=src/**/*.js --collectCoverageFrom=src/**/*.jsx --collectCoverageFrom=!src/**/*Styles.js --collectCoverageFrom=!src/**/index.js --collectCoverageFrom=!src/**/AppRoutes.js --collectCoverage true --coverageDirectory ./.coverage --watchAll=false",
    "test": "npm install && npm run lint && CI=true react-scripts test --env=jsdom",
    "localtest": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open --env MEUS_ANIMAIS_URL=http://localhost:3000",
    "cypress:run": "cypress run --env MEUS_ANIMAIS_URL=http://localhost:3000"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@sheerun/mutationobserver-shim": "^0.3.3",
